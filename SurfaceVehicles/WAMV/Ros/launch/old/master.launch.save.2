	</node>
-->
	<node
	  pkg="um7"
	  type="um7_driver"
	  name="imu" >
	<param name="port" value="/dev/imu" />
	</node>
	
	<node
	  pkg="rosserial_python"
	  type="serial_node.py"
	  name="serial"
	 >
	<param name="port" value="/dev/ttyACM1" />
	<param name="baud" value="57600" />
	</node>
