<launch>
   	<node
	  pkg="nmea_navsat_driver"
	  type="nmea_serial_driver"
	  name="gps"
	>
	<param name="port" value="/dev/ttyUSB0"/>
	<param name="baud" value="9600"/>

	</node>
	
	<node
	  pkg="usb_cam"
	  type="usb_cam_node"
	  name="usb_cam"
	>	
	<param name="autosize" value="true" />
	</node>
<!--
	<node
	  pkg="urg_node"
	  type="urg_node"
	  name="lidar">
